#include<iostream>
using namespace std;
int main(){
    int M;
    cin>>M;
    if( M%3==0 && M%5==0 )
    {
        cout<<"Good Number\n";
    }
    else if( M%3==0 && M%5!=0 ){
        cout<<"Bad Number\n";
    }
    else if( M%3!=0 && M%5==0 ){
        cout<<"Poor Number\n";
    }
    else{
        cout<<-1;
    }
}
